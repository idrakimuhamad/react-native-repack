{
  "name": "host-clone",
  "version": "0.0.1",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "android": "react-native run-android --no-packager",
    "ios": "react-native run-ios --no-packager",
    "pod_install": "cd ./ios && pod install",
    "start": "react-native webpack-start",
    "bundle": "react-native webpack-bundle --platform ios --entry-file index.js --dev=false",
    "webpack": "webpack -c webpack.config.js --env platform=ios --env mode=production"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "react": "18.1.0",
    "react-native": "0.70.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@callstack/eslint-config": "^12.0.2",
    "@callstack/repack": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/react-native": "^0.67.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.18.0",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.4.4",
    "webpack": "^5.73.0"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
